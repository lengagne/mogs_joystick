# - Config file for the MoGs_Joystick lib
# It defines the following variables
#
# MoGS_Joystick_INCLUDE_DIRS - include directories
# MoGS_Joystick_LIBRARY_DIRS - library directory (normally not used!)
# MoGS_Joystick_LIBRARIES - libraries to link against
 
# Tell the user project where to find our headers and libraries

set(MoGS_Joystick_INCLUDE_DIRS  @INSTALL_INCLUDE_DIRS@)

set(MoGS_Joystick_LIBRARY_DIRS  @INSTALL_LIB_DIRS@)

set(MoGS_Joystick_LIBRARIES MoGS_Joystick)

SET(MoGS_Joystick_FLAGS @MoGS_Joystick_FLAGS@)